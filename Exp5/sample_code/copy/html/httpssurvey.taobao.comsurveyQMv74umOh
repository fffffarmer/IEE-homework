(function() {
  var spma = 'a1z5c';
  var spmb = 'pc';
  var u = navigator.userAgent;
  var isAndroid = u.indexOf('Android') > -1 || u.indexOf('Adr') > -1; //android终端
  var isiOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端

  if (survey.xiniu) {
    spma = 'a211ab';
  }

  if (isAndroid || isiOS) {
    if (survey.xiniu) {
      if (isAndroid) {
        spmb = 'android';
      } else if(isiOS){
        spmb = 'ios';
      }
    } else {
      spmb = 'mobile';
    }
  }

  window.__WPO = {
      config: {
          //抽样分母，1代表100%上报，10代表10%上报，100代表1%上报
          sample: 1,
          // spmId必须设置,如果没写默认值,会自动抓页面的spm
          spmId: [spma, spmb].join('.'),
          // 是否启用动态配置
          dynamic: false
      }
  };
})()
