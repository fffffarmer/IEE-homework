            auto: true, //自动滚动
        });

        $('.indexEducation .indexEducationMain span').hover(function () {
            $(this).addClass('slideIn');
        }, function () {
            $(this).removeClass('slideIn');
        });

        $('.index_msfc').mousemove(function () {
            $('.index_msfc').removeClass('isHover');
            $(this).addClass('isHover');
        });

        $('.index_msfc').hover(function () {
            $('.index_msfc').removeClass('isHover');

        });


        $("#index_list1,#index_list2").xslider({
            unitdisplayed: 1,
            movelength: 1,
            unitlen: 360,
            autoscroll: 3000,
        });

        $("#index_list3").xslider({
            unitdisplayed: 1,
            movelength: 1,
            unitlen: 358,
            autoscroll: 3000,
        });

        // 客服
        $('#return').on('click', function () {
            $('body').scrollTop(0);
        });
        $('#open,#retract,.kefuSmall img').on('click', function () {
            $('#kefu').toggleClass('big small');
        });
    });

    function openurl() {
        window.location.href = "articleList.aspx?NewsTypeID=4";
    }

</script>
