//图片切换效果
	$(function(){
		var $imgBox = $(".imgBox"),
			img_w = $imgBox.width(),
			len = $imgBox.length,
			timer = 0,
			index = 1,
			animateTime = 500,
			hoverTimer, 
			outTimer;
			$(".imgBox").hover(function(){
				//index = $(this).index();
				//alert(index);
				  index=$(this).attr("id");
				
				clearTimeout(outTimer);
				hoverTimer = setTimeout(function(){
					for(var i=0;i<len;i++){
					if(i<=index){
						$imgBox.eq(i).animate({"left":20*i},animateTime);
					}else{
						$imgBox.eq(i).animate({"left":20*(i-1)+img_w},animateTime)
						}
					}},150)
				
				clearInterval(timer);
			},function(){
				clearTimeout(hoverTimer);
				outTimer = setTimeout(function(){
					timer = setInterval(function(){	
					$imgBox.eq(index).animate({"left":20*index},animateTime);
					index+=1;
					
					if(index>len){
						for(var i=1;i<len;i++){
							$imgBox.eq(i).animate({"left":20*(i-1)+img_w},animateTime)	
						}
						index=1;
					}
					
					},5000)
				},150)
			})
			
			timer = setInterval(function(){
				$imgBox.eq(index).animate({"left":20*index},animateTime);
				index+=1;
				console.log(index);
				if(index>len){
					for(var i=1;i<len;i++){
						$imgBox.eq(i).animate({"left":20*(i-1)+img_w},animateTime)	
					}
					index=1;
				}
				
			},4000)
			
			
	})	
	
/*顶部文字滚动*/
$(function(){
	var oTime = document.getElementById("time");
	var oUl = document.getElementById("ul1");
	var delay = 3000;
	var LiHeight = 20;
	var speed = 20;
	var time;
	oTime.innerHTML += oTime.innerHTML;
	oTime.scrollTop = 0;
	function startMove(){
		oTime.scrollTop++;
		time = setInterval(scrollUp,speed);
	}
	function scrollUp() {
		//area.scrollTop++;
		if(oTime.scrollTop % LiHeight==0){
			clearInterval(time);
			setTimeout(startMove,delay);
		}else{
			oTime.scrollTop++;
			if(oTime.scrollTop >= oTime.scrollHeight/2){    
				oTime.scrollTop = 0;
			}    //scrollHeight获取滚动内容的高度
		}
	}
	
	setTimeout(startMove,delay);//初始化	
})
	
