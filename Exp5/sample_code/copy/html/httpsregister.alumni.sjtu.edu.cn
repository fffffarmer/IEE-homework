<script src="https://register.alumni.sjtu.edu.cn/UI/js/login.js?v=2017-10-31" type="text/javascript"></script>
	<script type="text/javascript">
		var interval=0;
		$(document).ready(function(){
			setTimeout(function(){
				interval=setInterval("loginByQr()",1000);
			},200);
		});
		function loginByQr(){
			$.ajax({
				type:'post',
				url:'https://register.alumni.sjtu.edu.cn/scanLogin',
				data:'rd=176497153',
				dataType:'json',
				success:function(data){
					if(data.result!="error"){
						window.clearInterval(interval);
						if(data.result=="log"){
							$("div.jcloginNotice").html(data.msg);
							return false;
						}
						if(data.result=="success"){
							$("div.jcloginNotice").css("color","red").html("扫码成功<br/>正在登录...");
							//跳转
							if(top.location.href!=location.href){
								top.location.reload(); 
							}else{
								location.href = "https://register.alumni.sjtu.edu.cn/alumniHome";
							}
						}
					}
				}
			});
		}
	</script>
