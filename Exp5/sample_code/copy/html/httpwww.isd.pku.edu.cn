              "二十二","二十三","二十四","二十五","二十六","二十七","二十八","二十九","三十","三十一"];
    var monthE = ["JANUARY","FEBRUARY","MARCH","APRIL","MAY","JUNE","JULY","AUGUST","SEPTEMBER","OCTOBER","NOVEMBER","DECEMBER"];
    var json = [[{
                     date :"10",
                     url  :"",
                     title:"",
                     add :"",
                     time :"2016-10-08 16:00",
                     uri:""
                    }
               ],[{
                     date :"18",
                     montofyear :"9",
                     link :"",
                     title:"",
                     uri:"",
                     add :"",
                     time :"2016-10-08 16:00"
                    }]];
    
    //为Json对象添加date字段   
    function dealJson(){
       $.each(json,function(index,item){
            if (index<3){
                $.each(item,function(index,_item){
                    var time = _item["time"];
                    var regEx = new RegExp("\\-","gi"); 
                    time=time.replace(regEx,"/");
                    var date = new Date(time);
                    _item["date"] = date.getDate();
                });
            }
       });
    }
    function getJson(year,date){
        var url="http://115.27.240.128:8080/system/resource/calendarService.jsp?type=china&calendarselecteddate="+year+"-"+date;
        $.get(url,function(data,state){
            console.log(url);
            json = data;
           dealJson(); 
            calUtil.init(json[0]); 
        }, "json")
    }
  
    
    //更新通知信息
    function updateNotice(){
    var item=json[1][0]; 
        if (item.title!=undefined){
            $("#dayofmonth").html(item.date);
            $("#month").html(monthE[item.montofyear].substr(0 ,3));
              $("#title").text(item.title+" >>>");
             // $("#title").attr("href",item.link );
             $("#title").attr("href",item.uri);
              
        }
    }
    function updateCalName(date){
        //var calendarName=  +" "+calUtil.;
        
        $(".ui-datepicker-month").html(monthE[calUtil.showMonth-1]+" "); 
        $(".ui-datepicker-year").html(calUtil.showYear);
    }
    $(function(){  
        //ajax获取日历json数据  
        //calUtil.init([{"date":""}]);  
        
        //默认初始化
        calUtil.init([{"date":""}]);
        //使用服务器期时间戳,校正本地日历时间
        var url="/system/resource/calendarService.jsp?type=china";
        $.get(url,function(data,state,xhr){
            var datestr=xhr.getResponseHeader("Date");
            if(datestr){
                var date=new Date(datestr);
                if(date!=undefined){
                    calUtil.curYear=calUtil.showYear=date.getFullYear();
                    calUtil.curMonth=calUtil.showMonth=date.getMonth() + 1;
                    calUtil.curDate=calUtil.showDate=date.getDate();
                }
            }
            calUtil.getCurJson();
        },"json")

        
    }); 
//getJson(calUtil.showYear,calUtil.showMonth);
var calUtil = {
  //当前日历显示的年份
  showYear:2017,
  //当前日历显示的月份
  showMonth:1,
  //当前日历显示的天数
  showDate:1,
  showUrl:"",
  curYear:2017,
  curMonth:1,
  curDate:1,
  eventName:"load",
  curTab:0,
  //初始化日历
  init:function(signList){
  
    calUtil.setMonthAndDay();
    calUtil.draw(signList);
    calUtil.bindEnvent();
  },
  draw:function(signList){
  
  updateNotice();
    //绑定日历
    var str = calUtil.drawCal(calUtil.showYear,calUtil.showMonth,signList);
    $("#tab_info").html(str); 
    //绑定日历表头
    //var calendarName = buildCalName(terms[0],"三");
    //$(".ui-datepicker-month").html(calendarName);  
    if (calUtil.isCurrent()){
        updateCalName(parseInt(calUtil.curDate));
    }else{
        //var calendarName=  monthE[calUtil.showMonth-1]+" "+calUtil.showYear;
        $(".ui-datepicker-month").html(monthE[calUtil.showMonth-1]+" "); 
        $(".ui-datepicker-year").html(calUtil.showYear)
        //$(".ui-datepicker-month").html(calUtil.showMonth+"月"); 
    }
  },
  //绑定事件
  bindEnvent:function(){
    //绑定上个月事件
    $(".ui-icon-circle-triangle-w").click(function(){
      //ajax获取日历json数据
      calUtil.eventName="prev";
      //alert(calUtil.showMonth);
      calUtil.init(json[0]);
      calUtil.eventName="current";
      //getJson("2017","03");
      calUtil.getCurJson();
    });
    //绑定下个月事件
    $(".ui-icon-circle-triangle-e").click(function(){
    if(calUtil.showYear == calUtil.curYear && calUtil.showMonth == calUtil.curMonth)
    {}else{
      //ajax获取日历json数据
      calUtil.eventName="next";
      //alert(calUtil.showMonth);
      calUtil.init(json[0]);
      calUtil.eventName="current";
      //getJson("2017","04");
      calUtil.getCurJson();
      }
    });
   
  },
  //获取当前选择的年月
  setMonthAndDay:function(){
    switch(calUtil.eventName)
    {
      case "load":
        var current = new Date();
        calUtil.curYear=calUtil.showYear=current.getFullYear();
        calUtil.curMonth=calUtil.showMonth=current.getMonth() + 1;
        calUtil.curDate=calUtil.showDate=current.getDate();
        break;
      case "prev":
        var nowMonth=calUtil.showMonth;
        calUtil.showMonth=parseInt(nowMonth)-1;
        if(calUtil.showMonth==0)
        {
            calUtil.showMonth=12;
            calUtil.showYear-=1;
        }
        break;
      case "next":
        var nowMonth=calUtil.showMonth;
        calUtil.showMonth=parseInt(nowMonth)+1;
        if(calUtil.showMonth==13)
        {
            calUtil.showMonth=1;
            calUtil.showYear+=1;
        }
        break;
      case "current":
        break; 
    }
  },
      getCurJson:function(){
            var year = calUtil.showYear;
            var month = (calUtil.showMonth<10)?("0"+calUtil.showMonth):(""+calUtil.showMonth);
            getJson(year,month);
        },
  getDaysInmonth : function(iMonth, iYear){
   var dPrevDate = new Date(iYear, iMonth, 0);
   return dPrevDate.getDate();
  },
  bulidCal : function(iYear, iMonth) {
   var aMonth = new Array();
   aMonth[0] = new Array(7);
   aMonth[1] = new Array(7);
   aMonth[2] = new Array(7);
   aMonth[3] = new Array(7);
   aMonth[4] = new Array(7);
   aMonth[5] = new Array(7);
   aMonth[6] = new Array(7);
   var dCalDate = new Date(iYear, iMonth - 1, 1);
   var iDayOfFirst = dCalDate.getDay();
   //if(iDayOfFirst==0)iDayOfFirst=7;
   var iDaysInMonth = calUtil.getDaysInmonth(iMonth, iYear);
   var iVarDate = 1;
   var d, w;
   aMonth[0][0] = "Su";//"一";
   aMonth[0][1] = "Mo";//"二";
   aMonth[0][2] = "Tu"//"三";
   aMonth[0][3] = "We";//"四";
   aMonth[0][4] = "Th";//"五";
   aMonth[0][5] = "Fr";//"六";
   aMonth[0][6] = "Sa";//"日";
   for (d = iDayOfFirst; d < 7; d++) {
    aMonth[1][d] = iVarDate;
    iVarDate++;
   }
   for (w = 2; w < 7; w++) {
    for (d = 0; d < 7; d++) {
     if (iVarDate <= iDaysInMonth) {
      aMonth[w][d] = iVarDate;
      iVarDate++;
     }
    }
   }
   return aMonth;
  },
  ifHasSigned : function(signList,day){
   var signed = false;
   calUtil.showUrl ="";
   $.each(signList,function(index,item){
    if(item.date == day) {
     signed = true;
     calUtil.showUrl = item.url;
      calUtil.showUrl = item.uri;
     
     if(map[day]==undefined){
        map[day]=index;
     }
    }
   });
   return signed ;
  },
  isCurrent :function(){
   return (calUtil.curYear==calUtil.showYear && calUtil.curMonth==calUtil.showMonth);
  },
  drawCal : function(iYear, iMonth ,signList) {
   var myMonth = calUtil.bulidCal(iYear, iMonth);
   var htmls = new Array();
   htmls.push('<div id="datepicker" class="hasDatepicker"> ');
   htmls.push('<div class="ui-datepicker-inline ui-datepicker ui-widget ui-widget-content ui-helper-clearfix ui-corner-all" style="display: block;">');
   htmls.push('<div class="ui-datepicker-header ui-widget-header ui-helper-clearfix ui-corner-all">');
   htmls.push('<a class="ui-datepicker-prev ui-corner-all" data-handler="prev" data-event="click" title="Prev">');
   htmls.push('<span class="ui-icon ui-icon-circle-triangle-w">Prev</span></a>');
   
   if(calUtil.showYear == calUtil.curYear && calUtil.showMonth == calUtil.curMonth)
   {
   htmls.push('<a class="ui-datepicker-next ui-corner-all ui-state-disabled" title="Next">');
   }else{
   
    htmls.push('  <a class="ui-datepicker-next ui-corner-all" data-handler="next" data-event="click" title="Next">');
   }
  
   htmls.push('<span class="ui-icon ui-icon-circle-triangle-e">Next</span></a>');
     
   htmls.push('<div class="ui-datepicker-title">');
   htmls.push('<span class="ui-datepicker-month">December</span>');
   htmls.push('<span class="ui-datepicker-year">2017</span></div></div>');
   htmls.push('<table class="ui-datepicker-calendar">');
   htmls.push('<thead><tr>');
   htmls.push('<th class="ui-datepicker-week-end"><span title="Sunday">Su</span></th>');
   htmls.push('<th><span title="Monday">Mo</span></th>');
   htmls.push('<th><span title="Tuesday">Tu</span></th>');
   htmls.push('<th><span title="Wednesday">We</span></th>');
   htmls.push('<th><span title="Thursday">Th</span></th>');
   htmls.push('<th><span title="Friday">Fr</span></th>');
   htmls.push('<th class="ui-datepicker-week-end"><span title="Saturday">Sa</span></th>');
   htmls.push('</tr></thead><tbody>');
   htmls.push('');   
  
   var d, w;
   for (w = 1; w < 7; w++) {
    htmls.push("<tr>");
    for (d = 0; d < 7; d++) {
     var ifHasSigned = calUtil.ifHasSigned(signList,myMonth[w][d]);
         
     //当前天 ui-datepicker-today
     //有文章可以点击 没有样式
     if(d==0 || d==6)
     {
         if(ifHasSigned)
         {
             htmls.push('<td class="ui-datepicker-week-end undefined">');   
         }else
         {
             htmls.push('<td class="ui-datepicker-week-end ui-datepicker-unselectable ui-state-disabled undefined">');  
         }
   
     }else{
     
          if(ifHasSigned)
         {
                htmls.push('<td class=" undefined">');
         }else
         {
         htmls.push('<td class="ui-datepicker-unselectable ui-state-disabled undefined">');
         }
     
     }
     if(ifHasSigned){
     
    // var item=json[calUtil.curTab][map[d]];
     //var uri = item.url;
     var uri =calUtil.showUrl;
     
      htmls.push((!isNaN(myMonth[w][d]) ? '<a class="ui-state-default" href="'+uri+'">'+myMonth[w][d]+'</a>' : " ") +'</td>');
     }else{
      htmls.push((!isNaN(myMonth[w][d]) ? '<span class="ui-state-default">'+myMonth[w][d]+'</span>' : " ") +'</td>');
     }
    
    
    }
    htmls.push("</tr>");
   }
   htmls.push("</tbody>");
   htmls.push("</table>");
   htmls.push("</div>");
   htmls.push("</div>");
   return htmls.join('');
  }
}; 
