<script>
    function getQueryString(name) {
      var url = window.location.href;
      var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
      var index = url.indexOf('?');
      var r = url.substr(index).substr(1).match(reg);  //获取url中"?"符后的字符串并正则匹配
      var context = '';
      if (r != null)
        context = r[2];
      reg = null;
      r = null;
      return context == null || context == '' || context == 'undefined' ? '' : context;
    }
    var byUrl = JSON.parse('[{"urlParamKey":"pm","urlParamValue":"true","moduleName":"paiheader-ems"},{"urlParamKey":"pm","moduleName":"secondhand-car-head"}]');
    for (var i = 0; i < byUrl.length; i++) {
      var current = byUrl[i];
      var actualValue = getQueryString(current.urlParamKey);
      var targetValue = current.urlParamValue;
      if (actualValue === targetValue || (actualValue === '' && targetValue === undefined)) {
        document.body.className += ' ' + current.moduleName + '-show';
      }
    }

  </script>
