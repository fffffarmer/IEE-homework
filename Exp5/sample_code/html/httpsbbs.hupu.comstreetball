$(".it-search-for").bind('input propertychange', function() {
    var $value=$(".it-search-for").val();
  if($value.length>0){
      $(".hp-search-choose").show();
      $(".searchval").html($value);
  }else{
      $(".hp-search-choose").hide();
  }
