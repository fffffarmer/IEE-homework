function update_vote_bbs(vid, rand) {
    $('#innervote' + rand + ' input.btns2').attr("disabled", true).after('<b class="loading">&nbsp;</b>');
    if ($('#voteclass' + vid).val() > 1) {
        var chk_value = '',
            votename = "vote" + rand;
        $('input[name^=' + votename + ']:checked').each(

        function () {
            var this_val = $(this).val();
            chk_value += '' + this_val + '|';
        });
    } else {
        var votename = "vote" + rand,
            chk_value = $("input[name=" + votename + "]:checked").val();
    }
    var fuvoteid = vid < 0 ? vid : 0 - vid;
    $.getJSON('https://snsvote.hupu.com/InsertVote_new.php?enews=InsertVote&voteid=' + vid + '&vote=' + chk_value + '&jsoncallback=?', function (json) {
        $('#innervote' + rand + ' input.btns2').attr("disabled", false);
        $("b.loading").remove();
        if (json.code == 1) {
            $.getJSON('https://snsvote.hupu.com/update_vote_new.php?showtype=bbs&voteid=' + vid + '&jsoncallback=?', function (data) {
                $('#innervote' + rand).html(data.info);
            });
        } else {
            show_message("<div class='tips_pop'>" + json.info + "</div>");
        }
    });
}

function tipfight(tid,pid){
	var tid = tid,fid = 155,fidname='中国足球',state=1,authorid = $("#tipcolor"+pid).attr("mid"),token='4306b06d0c';
	$.ajax({
		type: "POST",
		dataType: "json",
		url: "/ajax/lights.ajax.php?" + Math.random(),
		data: {tid:tid, pid:pid, state:state, authorid:authorid, fid:fid,token:token},
		success: function(data) {
			$("b.loading").hide();
			if(data.code != 1){
				if(data.code == -1){
					show_login();
				}else{
					show_message("<div class='tips_pop'>"+data.msg+"</div>");
				}

				return false;
			}else{
				$("#tip"+pid).text(data.num);
				if(state==1){
					$("#tipcolor"+pid).attr("class","red");
				}else{
					$("#fightcolor"+pid).attr("class","f666");
				}
				ga("bbs-c-highlights-click");
			}
		}
	});
}
