    if(0==1){
          harf='qanda-digest'
    }else{
      if(5>0){
        if(postdate>0){
          harf='qanda-type5-postdate'
        }else{
          harf='qanda-type5'
        }
      }else{
        if(postdate>0){
          harf='qanda-postdate'
        }else{
          harf='qanda'
        }
      }

    }
    $(".for-list li").mouseover(function(event) {
      $(this).find(".caozhuo").css("visibility","visible");
    }).mouseout(function(){
      $(this).find(".caozhuo").css("visibility","hidden");
    });
    if(1>1){
      $(".downpage").css("margin-left","-185px");
    }
    if(1>=5){
      $(".downpage").css("margin-left","-194px");
    }


    $('.page').createPage(function(n){

    },{
    	pageCount:maxpage,//总页码,默认10
    	current:1,//当前页码
      name:harf+'-',//标记
      hname:'qanda',
    	showNear:1,//显示当前页码前多少页和后多少页，默认2
    	pageSwap:true,
      align:'right',
    	showSumNum:false,//是否显示总页码
      maxpage:100,
      is_poslist:1,
      is_read:0

    });
    $(".nextPage").css({
      "height":"16px",
      "line-height":"16px"
    });
