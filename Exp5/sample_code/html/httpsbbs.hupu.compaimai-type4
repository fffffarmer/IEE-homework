    var maxpage=0;
    if(78>100){
      maxpage=100;
    }else{
      maxpage=78;
    }
    var harf='';
    var loc=location.pathname.slice(1);
    var postdate=location.pathname.indexOf('postdate')|| -1;
    if(0==1){
          harf='paimai-digest'
    }else{
      if(4>0){
        if(postdate>0){
          harf='paimai-type4-postdate'
        }else{
          harf='paimai-type4'
        }
      }else{
        if(postdate>0){
          harf='paimai-postdate'
        }else{
          harf='paimai'
        }
      }

    }
    $(".for-list li").mouseover(function(event) {
      $(this).find(".caozhuo").css("visibility","visible");
    }).mouseout(function(){
      $(this).find(".caozhuo").css("visibility","hidden");
    });
    if(1>1){
      $(".downpage").css("margin-left","-185px");
    }
    if(1>=5){
      $(".downpage").css("margin-left","-194px");
    }


    $('.page').createPage(function(n){

    },{
    	pageCount:maxpage,//总页码,默认10
    	current:1,//当前页码
      name:harf+'-',//标记
      hname:'paimai',
    	showNear:1,//显示当前页码前多少页和后多少页，默认2
    	pageSwap:true,
      align:'right',
    	showSumNum:false,//是否显示总页码
      maxpage:78,
      is_poslist:1,
      is_read:0

    });
