__ready.push(function(){//搜索按钮
$(".lit-search-tit").bind('input propertychange', function() {
});
$(".lit-search-btn").click(function(){
 var $value=$(".lit-search-tit").val();
 console.log($value)
 window.open('//my.hupu.com/search?fid=3008&type=topic&q='+$value,'_blank');
})

// 回车键事件
$(".lit-search-tit").keypress(function(e) {
 if(e.which == 13) {
   var $value=$(".lit-search-tit").val();
 window.open('//my.hupu.com/search?fid=3008&type=topic&q='+$value,'_blank');
 }
});


});
