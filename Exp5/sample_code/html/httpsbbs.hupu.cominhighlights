__ready.push(function(){var apiUrl = {
      //点亮
      set_light:'/set_light'
};
//点亮
$(".ilike_icon").click(function(event) {
  var lighturl=apiUrl.set_light;
  var pid=$(this).attr("data-pid");
  var state=1,authorid = $("#tipcolor"+pid).attr("mid"),token=$(".token").val();
  var tid=$(this).attr("data-tid");
  var fid=$(this).attr("data-fid");
  $.ajax({
    type: "POST",
    dataType: "json",
     //url: lighturl,
     url: "https://bbs.hupu.com/ajax/lights.ajax.php?" + Math.random(),
    data: {
      tid:tid,
      pid:pid,
      state:state,
      authorid:authorid,
      fid:fid,
      token:token
    },
    success: function(data) {
       console.log(data);
       if(data.code != 1){
         $(".tips_up_pop").fadeIn(300).fadeOut(6000);
         $(".tips_pop").text(data.msg)
       }else{
          $("#tip"+pid).text(data.num);
          if(state==1){
           $("#tipcolor"+pid).attr("class","red");
           }else{
               $("#fightcolor"+pid).attr("class","f666");
           }
       }

    }
  });
});
});
__ready.push(function(){
var maxpage=0;
if(99>99){
  maxpage=99;
}else{
  maxpage=99;
}

  $('.page').createPage(function(n){
    if(n=maxpage){
      $(".page").css("margin-left","0");
    }
  },{
    pageCount:maxpage,//显示页码
    maxpage:99,//实际页码
