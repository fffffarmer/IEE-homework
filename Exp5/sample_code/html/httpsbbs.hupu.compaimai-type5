    var maxpage=0;
    if(100>100){
      maxpage=100;
    }else{
      maxpage=100;
    }
    var harf='';
    var loc=location.pathname.slice(1);
    var postdate=location.pathname.indexOf('postdate')|| -1;
    if(0==1){
          harf='paimai-digest'
    }else{
      if(5>0){
        if(postdate>0){
          harf='paimai-type5-postdate'
        }else{
          harf='paimai-type5'
        }
      }else{
        if(postdate>0){
          harf='paimai-postdate'
        }else{
          harf='paimai'
        }
      }

    }
    $(".for-list li").mouseover(function(event) {
      $(this).find(".caozhuo").css("visibility","visible");
    }).mouseout(function(){
      $(this).find(".caozhuo").css("visibility","hidden");
    });
    if(1>1){
      $(".downpage").css("margin-left","-185px");
    }
    if(1>=5){
      $(".downpage").css("margin-left","-194px");
    }


    $('.page').createPage(function(n){

    },{
    	pageCount:maxpage,//总页码,默认10
