              tab+="<div class='txt-link'><a class='tit' href='"+data.data[0].link+"' target='_blank'>"+data.data[0].title+"</a><a class='txt' href='"+data.data[0].link+"' target='_blank'>"+data.data[0].content+"</a></div>";
            }
            tab+="<a href='"+data.data[0].src+"' class='tc-more' target='_blank'>查看更多</a></div>";
            $('body').append(tab);
      }
        } 
    $("#tc-adv .tc-close").click(function(){
      $("#tc-adv").slideUp(1200);
    });
    cookie.set("game-popup",data.data[0].stime,"1day");
  }
})();
</script>   
