!function(){        require.async(['common:widget/ui/jquery/jquery.js', 'common:widget/ui/jquery/jquery.cookie.js'], function($){
           var $ca_localname = $("#ca_localname");
           var $chooseicon = $(".choose-icon");
            var oCa_areas = getShowAndHidden("ca_areas",{
                beforeshow : function(){
                    $ca_localname.addClass("localshow");
                    $chooseicon.removeClass("icon-hide");
                    $chooseicon.addClass("icon-show");
                },
                beforehide : function(){
                    $ca_localname.removeClass("localshow");
                     $chooseicon.removeClass("icon-show");
                    $chooseicon.addClass("icon-hide");
                }
            });
            
           $("#ca_localname, .local-icon, .choose-icon").bind("click",function(event) {
                oCa_areas.click();
            });

            $("body").bind("mousedown",function(e){
                var target = e.target|| e.srcElement;

                if(!$(target).parents(".ca-local").length){
                  oCa_areas.hide();
                }
            });



            function getShowAndHidden(id,option){
                var $id = $("#" + id);
                return {
                    hide : function(){
                        option.beforehide();
                        $id.hide();
                    },
                    show : function(){
                        option.beforeshow();
                        $id.show();
                    },
                    click : function(){
                        var flag = $id.css("display");
                        if(flag == "none"){
                            this.show();
                        }
                        else{
                            this.hide();
                        }
                    }
                }
            } 
            window.setLocCookie = function(name,value){
                var options = {
                    "domain" : "news.baidu.com",
                    "expires" : 30*1000*60*60*24
                };
                $.cookie(name, value,options);
            }
        });
    }();
