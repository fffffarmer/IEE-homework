!function(){      var Slide = require('common:widget/ui/vs/Slide/Slide.js');
         var slide = new Slide(
			{
				container: ".widget-submenu",
				item: "a",
				defaultClass: "underline",
				defaultBackClass: "subnavcurrent",
				time: 200,
				timer: 20,
				bezier:[{x: 0.42,y: 0.6},{x: 0.29,y: .62}],
				onMove: function(s, t){
					if(this.supportCss3("transition-timing-function")){
						$(s).width($(t).width() + 10);
					}
					else{
						window.clearInterval(window.onmoveInterval);
						var sw = $(s).width();
						var tw = $(t).width() + 10;

						var dis = tw - sw;
						var i = 0, l = 20;
						var onmoveInterval = setInterval(function(){
							$(s).width(sw + dis / l * ++i );

							if(i >= l){
								window.clearInterval(onmoveInterval);
							}
						},10);
						window.onmoveInterval = onmoveInterval; 
					}
				}
			}
		);

		$underline = $(".underline");
		$(".widget-submenu").hover(function() {
			$underline.css("visibility","visible");
		}, function() {
			$underline.css("visibility","hidden");
		});

    }();
