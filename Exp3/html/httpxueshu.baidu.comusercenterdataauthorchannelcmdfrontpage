var get = function(url) {
		if(location.protocol === "http") {
			return url;
		}
		var reg = /^(http[s]?:\/\/)?([^\/]+)(.*)/,
			reg2 = /^(http[s]?:\/\/)?(s1.bdstatic.com\/r\/www\/cache\/mid\/static)(.*)/,
		matches = url.match(reg),
		//s1.bdstatic.com静态域名特殊处理
		matches2 = url.match(reg2);
		if(matches2){
			url = "https://ss1.bdstatic.com/5eN1bjq8AAUYm2zgoY3K/r/www/cache/mid/static/protocol/https"+ matches2[3];
			return url;
		}else{
			//判断传入参数是域名还是地址，分别做处理
			url = list.hasOwnProperty(matches[2])&&(list[matches[2]] + matches[3]) || url;
			return url;
		}	
	},
	set = function(kdomain,vdomain) {
		list[kdomain] = vdomain;
	}; 
	return {
		get : get,
		set : set
	}
})();
