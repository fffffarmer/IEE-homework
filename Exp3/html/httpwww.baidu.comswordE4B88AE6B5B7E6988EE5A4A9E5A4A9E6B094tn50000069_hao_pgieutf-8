	var hashMatch = document.location.href.match(/#+(.*wd=[^&].+)/);

	if (hashMatch && hashMatch[0] && hashMatch[1]) {
		document.location.replace("http://"+location.host+"/s?"+hashMatch[1]);
	}

	//结果页js命名空间
	var bds = {
		comm:{
        	loginAction : []
		},
		se:{},
		su:{
				urdata:[],
			urSendClick:function(){}
		},
