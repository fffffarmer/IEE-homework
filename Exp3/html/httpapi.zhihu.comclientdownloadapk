function createFrame(u){
window.location.replace(u);
//var obj = document.createElement('iframe');obj.style.height = '1px';obj.style.display = 'none';obj.src = u;
//document.body.appendChild(obj);return obj;
setTimeout(function () {
if (window.location.href == u) {
var n = document.getElementById('prompt'); n.textContent = '跳转成功';
}
}, 300);
}
function getParameterByName(name, url) {
if (!url) {
url = window.location.href;
}
name = name.replace(/[\[\]]/g, "\\$&");
var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
results = regex.exec(url);
if (!results) return null;
if (!results[2]) return '';
return decodeURIComponent(results[2].replace(/\+/g, " "));
}
(function() {
var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
var za = document.createElement('script'); za.type = 'text/javascript'; za.async = true;
za.src = 'http://static.analytics.zhihu.com/static/za.js?v=4';
s.parentNode.insertBefore(za, s);
var isload = false;
function load() {
if (isload) return;
isload = true;
createFrame('https://pkg.zhimg.com/zhihu/futureve-mobile-apk-release-5.27.1(933).apk');
}
function checkIfAnalyticsLoaded() {
if (((window._gat && window._gat._getTracker) || window.urchinTracker) && isTrackUrlLoaded) {
load();
} else {
// Probably want to cap the total number of times you call this.
setTimeout(function () {checkIfAnalyticsLoaded();}, 300);
}
}
