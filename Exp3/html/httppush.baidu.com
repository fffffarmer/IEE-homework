                $('.fixeds').css('bottom', '140px');
    		}
    		else {
    			qrcode.hide();
    			qrcodeIcon.show();
                $('.fixeds').css('bottom', '20px');
    		}
    	}

    	closeIcon.click(function () {
    		codeStatus = false;
    		qrcodeToggle();
    	});

    	qrcodeIcon.click(function () {
    		codeStatus = true;
    		qrcodeToggle();
    	});

    	qrcodeToggle();
    });
</script>
