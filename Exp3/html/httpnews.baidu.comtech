        window.callback = function(oder,opts){
            var wrapper = oder == 1 ? document.getElementById('mobile-price') : document.getElementById('pc-price'),
                    temp = [],
                    html = '<ul class="olist">';
            for(var i in opts){
                if(i < 7){
                    opts[i]['index'] = i;
                    opts[i]['name'] = baidu.string.subByte(opts[i]['name'], 28);
                    temp.push(i > 3 ? baidu.string.format(tpl, opts[i]) : baidu.string.format(h_tpl, opts[i]));
                }
            }
            wrapper.innerHTML = html + temp.join('') + '</ul>';
        }
        baidu.sio.callByServer('https://detail.zol.com.cn/corp_CorpBaidu_subcateId=57^showNum=8^type=top.html');
