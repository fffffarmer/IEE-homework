!function(){    require.async(["ydcommon:widget/lib/tangram/base/base.js", "ydcommon:widget/ui/js_core/login/login.js", "ydcommon:widget/ui/js_core/dropDown/dropDown.js"], function (T, login, dropDown) {
        T.dom.ready(function(){
            var TE = T.event,
                G = T.g;

            var pathName = document.location.pathname,
            navTest = {
                "nav-index":/^\/$/i,
                "nav-category":/^\/book\/list/i,
                "nav-rank":/^\/rank/i,
                "nav-author":/^\/author/i,
                // "nav-activity":/^\/activity\/index/i,
                "nav-vip":/^\/vip/i,
                "nav-mobile" : /^\/apps/i,
                "nav-org": /^\/partner\/index/i
            },
            _login = login.login;

            for(var item in navTest){
                if ( navTest.hasOwnProperty( item ) ) {
                    var tmp = navTest[item];
                    if(tmp && tmp.test && tmp.test(pathName)){
                        $('.'+item).parent().addClass('current');
                    }
                }
            }

            // add by lizhaoji01
            $('.nav-right-mybook').on('click', function(e) {
                e.preventDefault();
                login.login.verlogin(function(){
                    window.location.href = '/customer/mybook?fr=head_nav';
                });
            });

        });
    });
}();
