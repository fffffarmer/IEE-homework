	function renderNews() {
				var strArr = [];
				if ($.isArray(hotNews)) {
					for (var i = 0, l = hotNews.length; i < 8; i++) {
						var obj = hotNews[i];
						var iconStr = obj['isnew'] + "" == "1" ? "<span class='icon_new'></span>" : "";
						strArr.push("<li><span><a target='_blank' href='" + obj['href'] + "'>" + obj['text'] + iconStr + "..............................................</a><span> (" + obj['time'] + ") </span></span></li>");
					}

					$("#dynamics-con").html(strArr.join(""));
				}
			}
	
	renderNews();
