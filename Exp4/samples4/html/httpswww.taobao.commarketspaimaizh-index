<script type="text/javascript">
(function () {
    var $ = function () {
        var elements = new Array();
        for (var i = 0; i < arguments.length; i++) {
            var element = arguments[i];
            if (typeof element == 'string')
                element = document.getElementById(element);
            if (!element) continue;
            if (arguments.length == 1) return element;
            elements.push(element);
        }
        return elements;
    };
    var setOpacity = function (node, level) {
        node = $(node);
        if (document.all) {
            node.style.filter = 'alpha(opacity=' + level + ')';
        } else {
            node.style.opacity = level / 100;
        }
    };
    var getTag = function (child, parent) {
        return parent.getElementsByTagName(child);
    };
    var bindEvent = function (element, type, func) {
        if (element.addEventListener) {
            element.addEventListener(type, func, false);
        } else if (element.attachEvent) {
            element.attachEvent('on' + type, func);
        } else {
            element['on' + type] = func;
        }
    };
    function Slider(options) {
        var id = options.id;
        var warper = this.warper = $(id);
        var warpLis = this.warpLis = getTag("li", warper);
        this.no = warpLis.length;
        this.step = options.step || 2;
        this.autoTime = options.autoTime || 1000;
        this.btnId = options.btnId || "focus_btn";
        this.index = 1;
        this.preIndex = 0;
        this.init();
    }
    Slider.prototype = {
        init:function () {
            this.makeBtn(this.no, this.btnId);
            this.autoPlay();
        },
        makeBtn:function (no, c) {
            var btnUl = this.btnUl = document.getElementById(c);
            var lis=btnUl.getElementsByTagName('li');
            for (var i = 0; i < no; i++) {
                if (i == 0) lis[i].className = 'on';
                //setOpacity(lis[i], 80);
                bindEvent(lis[i], 'mouseover', function (obj, t) {
                    return function () {
                        obj.mouseOn.call(obj, t);
                    }
                }(this, i));
            }
        },
//      makeBtn:function (no, c) {
//          var btnUl = this.btnUl = document.createElement("ul");
//          btnUl.id = c;
//          for (var i = 0; i < no; i++) {
//              var li = document.createElement('li');
//              if (i == 0) li.className = 'on';
//              var text = document.createTextNode(i + 1);
//              li.appendChild(text);
//              setOpacity(li, 80);
//              bindEvent(li, 'mouseover', function (obj, t) {
//                  return function () {
//                      obj.mouseOn.call(obj, t);
//                  }
//              }(this, i));
//              btnUl.appendChild(li);
//          }
//          this.warper.parentNode.appendChild(btnUl);
//      },
        autoPlay:function(){
            var that = this;
            clearTimeout(this.T1);
            this.T1 = setTimeout(function(){that.fadeIn(that.index)},that.autoTime);
        },
        fadeIn:function(index){
            var thisObj = this.warpLis[index];
            var thisOpacity = 0;
            var that = this;

            var btnLi = getTag('li', this.btnUl);
            for (var i = 0, n = btnLi.length; i < n; i++) {
                btnLi[i].className = '';
                this.warpLis[i].style.zIndex = '';
            }
            setOpacity(this.warpLis[index],0);
            this.warpLis[that.preIndex].style.zIndex = 1;
            this.warpLis[index].style.zIndex = 2;
            btnLi[index].className = 'on';
            if(that.T2)clearInterval(that.T2);
            this.T2 = setInterval(function(){
                setOpacity(thisObj,thisOpacity++);
                if(thisOpacity == 100){
                    thisOpacity = 0;
                    clearInterval(that.T2);
                    that.T2 = null;
                    that.preIndex = that.index ++;
                    if(that.index == that.no)that.index = 0;
                    that.autoPlay();
                }
            },10);
        },
        mouseOn:function(index){
            this.index = index;
            this.fadeIn(index);
        }
    };
    if(document.getElementById('focus_pic1').getElementsByTagName('img').length>1){
    var slider1 = new Slider({
        id:'focus_pic1',
        btnId:"focus_btn1",
        autoTime:2000
    });
    }
})();
</script>
