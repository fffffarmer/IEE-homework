<script type="text/javascript">
    var aaa;
    var Number = 1; //当前第几张
    var divNumber = $("div[name='FDTP_Div']").length; //一共多少张

    //显示图片
    function ShowImage(position) {
        $("div[name='FDTP_Div']").hide();
        $("#FDTP_" + position).show();
        $("div[name='title']").hide();
        $("#title_" + position).show();
    }
    //上一张
    function NextImage() {
        stop();
        if (Number > 1) {
            Number--;
        }
        else {
            Number = divNumber;
        }
        ShowImage(Number);
        start();
    }
    //下一张
    function UpImage() {
        stop();
        if (Number < divNumber) {
            Number++;
        }
        else {
            Number = 1;
        }
        ShowImage(Number);
        start();
    }

    function start() { aaa = window.setInterval("NextImage();", 5000); }
    function stop() { window.clearInterval(aaa); }

    $(document).ready(function () {
       

        //翻动图片显示第一张
        ShowImage(Number);

        //上一张
        $("#UpImage").click(function () {
            NextImage();
        });

        //下一张
        $("#NextImage").click(function () {
            UpImage();
        });

        //显示左右箭头
        $(".slider").mousemove(function () {
            $(".jt").show();
        });

        //隐藏左右箭头
        $(".slider").mouseout(function () {
            $(".jt").hide();
        });

        start();
    });

</script>