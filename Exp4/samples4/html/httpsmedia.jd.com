  <script type="text/javascript">
$(function(){
    var bodyWidth = $("body").width();
      if(bodyWidth<1920){
         var ml=(1920-bodyWidth)/2;
         $(".banner").find(".img img").css("margin-left",-ml);
      }

      $('#slides-banner').bxSlider({
        slideWidth: 1920,
        minSlides: 1,
        maxSlides: 1,
        auto: true,
        autoControls: false,
        slideMargin: 0
      });

      $('#slides-fri').bxSlider({
        slideWidth: 156,
        minSlides: 1,
        maxSlides: 8,
        auto: true,
        autoControls: false,
        slideMargin: 0
      });
       indexObj.init();
});
var indexObj = {
    httpConfig: location.origin,
    init: function() {
      	 if (window["context"] == undefined) {
	        if (!window.location.origin) {
	            window.location.origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port : '');
	        }
	        window["context"] = location.origin + "/V6.0";
	     }
      	 this.render();
        var inviteCode =  getUrlPar("inviteCode");
        var isHide = getUrlPar("isHide");
        var extendedParameter = getUrlPar("extendedParameter");
        var showCodeLoginUrl = "//passport.jd.com/common/loginPage?from=media&ReturnUrl=" + encodeURIComponent(location.origin+"/loginJump");
        $("#indexIframe").attr( "src", !inviteCode ? showCodeLoginUrl : showCodeLoginUrl +  encodeURIComponent("?inviteCode=" + inviteCode +  "&isHide=" + isHide + "&extendedParameter=" + extendedParameter ) );
    },
    render: function() {
        var _this = this;
        //公告
        $.ajax({
            type: "GET",
            url: _this.httpConfig + "/getIndexNoticeJsonpList",
            dataType: "jsonp",
            cache: false,
            success: function(data) {
                //console.log(data);
                var lmNoticeTitle = $('#lmNoticeTitle');
                for (var i = 0; i < data.length; i++) {
                    if (i == 3) {
                        return false
                    }
                    lmNoticeTitle.append('<li><a href="//media.jd.com/notice/info/' + data[i].noticeId + '" target="_blank">' + data[i].title + '</a><span class="notice-time">' + data[i].releaseTime + '</span></li>');
                }
            }

        });
        //判断是否登陆
        if (cookie("pin") == null || cookie("pin") == "" || cookie("pin") == "undefined") {
            _this.noLogin();
        } else {
            $.ajax({
                type: "GET",
                url: _this.httpConfig + "/isloginJsonp",
                dataType: "jsonp",
                cache: false,
                success: function(data) {
                    if (data == "logout") {
                        _this.noLogin();
                    } else if (data == "login") {
                        _this.isLogin();
                        $("#userPin").html(decodeURIComponent(getCookie("pin")));
                        $.ajax({
                            type: "GET",
                            url: _this.httpConfig + "/user/myunion/jsonpMsg",
                            dataType: "jsonp",
                            cache: false,
                            success: function(data) {
                                $(".inBtn").text("进入联盟").off().attr("href", _this.httpConfig+"/index/overview");
                                $("#msgTag h3").removeClass("ishide");
                                $(".loginBottom,#lmMsg").show();
                                var lmMsgTitle = $('#lmMsgTitle');
                                for (var i = 0; i < data.length; i++) {
                                    if (i == 3) {
                                        return false
                                    }
                                    lmMsgTitle.append('<li><a href="//media.jd.com/user/myunion/msg" target="_blank">' + data[i].messageTitle + '</a></li>');
                                }
                            },
                            error: function() {
                                //无账户
                                _this.inviteCodeInit();
                                $(".inBtn").text("开通京东联盟");
                                $("#msgTag h3").addClass("ishide");
                                $(".loginBottom,#lmMsg").hide();
                            }
                        });
                    }
                },
                error: function() {
                    _this.noLogin();
                }

            });

        }
        this.bindEvent();
    },
    bindEvent: function() {
        var _this = this;
        var clickObj = $("#msgTag h3").find("span");
        clickObj.on("click", function() {
            var index = $(this).index();
            $(this).addClass("active").siblings("span").removeClass("active");
            $("#msgTag").find("ul").eq(index).show().siblings("ul").hide();
        });
        $("#iRegister").on("click", function() {
            window.location.href = "//reg.jd.com/reg/person?ReturnUrl=" + encodeURIComponent("//" + window.location.href);
        });

        $(".inBtn").on("click", function () {
            event.preventDefault(); // 阻止默认事件
            event.stopPropagation(); // 阻止冒泡
            var unionYqmId = $("#unionYqmId").val();
                $.ajax({
                    async : false,
                    type : "get",
                    dataType: "jsonp",
                    url :_this.httpConfig+ "/unionRegister?yqmUnion=" + unionYqmId + "&extendedParameter=" + (_this.extendedParameter ? _this.extendedParameter : ""),
                    success : function(data) {
                        if (data.code != 500) {
                            if(data.code == 100){
                                window.location.href =  _this.httpConfig + "/logout";
                            }else if(data.code == 200){
                                window.location.href = _this.httpConfig + "/index/overview";
                            }else{
                                alert(data.msg);
                            }
                        } else {
                            alert('亲，操作失败，请稍后再试！');
                        }
                    },
                    error : function() {
                        alert("代码出错，请稍后再试！");
                    }
                });
        });


        $(".pro-ul li,.ads").on("click", function() {
            var $this = this;
            var urls = $($this).find("a").eq(0).attr("iherf");
            if (cookie("pin") == null || cookie("pin") == "" || cookie("pin") == "undefined") {
                location.href = "//passport.jd.com/new/login.aspx?ReturnUrl=" + encodeURIComponent(urls);
            } else {
                $.ajax({
                    type: "GET",
                    url: _this.httpConfig + "/isloginJsonp",
                    dataType: "jsonp",
                    cache: false,
                    success: function(data) {
                        if (data == "logout") {
                            location.href = "//passport.jd.com/new/login.aspx?ReturnUrl=" + encodeURIComponent(urls);
                        } else if (data == "login") {


                            $.ajax({
                                type: "GET",
                                url: _this.httpConfig + "/user/myunion/jsonpMsg",
                                dataType: "jsonp",
                                cache: false,
                                success: function(data) {

                                    location.href = urls;
                                },
                                error: function() {
                                    location.href = "//media.jd.com";
                                }
                            });


                        }
                    }
                });



            }
