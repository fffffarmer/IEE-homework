       function formatDateTime(Date) {
        var _year = Date.getFullYear();
        var _month = Date.getMonth();
        var _date = Date.getDate();
        _month = _month + 1;
        if(_month < 10){_month = "0" + _month;}
        if(_date<10){_date="0"+_date  }
        return  _year + "-" + _month + "-" + _date;
       }
        
        function parseArr(data){
            var nameArr=[];
            var result=[];
            data.forEach(function(item) {
                var i;
                if((i = nameArr.indexOf(item.name))>-1){
                    result[i].value=Number(result[i].value)+Number(item.value);
                }else{
                    nameArr.push(item.name);
                    result.push({
                        name:item.name,
                        value:item.value
                    })
                }
            })
            return result;
        }
        
        function getTimeArr(json,data) {
            var timeArr = new Array();
            for(var i = 0; i < json.length; i++) {
                if(json[i]) {
                    var s = json[i].jzsjRq;
                    //s = new Date(s);
                    //s = formatDateTime(s);
                    var ss = s.split(" ").length > s.split("$nbsp;").length ? s.split(" ") : s.split("$nbsp;");
                    timeArr[i] = ss[0];
                    if(json[i].jzdd.length != 0){
                       if (json[i].jzdd.indexOf("徐汇") != -1) {
                           var XuhuiRow = {
                               name : "徐汇校区",
                               value : 1
                           }
                           data.push(XuhuiRow);
                           var row = {
                               name : json[i].jzdd,
                               value : 1
                           };
                           data.push(row);
                       }
                       if (json[i].jzdd.indexOf("闵行") != -1) {
                           var MinhangRow = {
                               name : "闵行校区",
                               value : 1
                           }
                           data.push(MinhangRow);
                           var row = {
                               name : json[i].jzdd,
                               value : 1
                           };
                           data.push(row);
                       }
                    }
                }
            }
            return timeArr;
        }
        
        function eqDate(timeArr){
            $(".datetimepicker-days .table-condensed tbody td").addClass("disabled");
            $(".datetimepicker-days .table-condensed tbody td").removeClass("active");
            for(var i=0;i<timeArr.length;i++){
                $(".datetimepicker-days .table-condensed tbody td").each(function(){
                    var switchTime = $(".switch").html();
                    var switchTimeArr = switchTime.split(" ");
                    var sMonth = parseInt(switchTimeArr[1]);
                    var sYear = parseInt(switchTimeArr[3]);
                    var sDay = $(this).html();
                    if(sDay.length == 1){
                        sDay = 0 + sDay;
                    }
                    if($(this).hasClass("old")) {
                        sMonth = sMonth -1;
                        if (sMonth==0) {
                            sMonth = 12;
                            sYear = sYear - 1;
                        }
                        if (sMonth<10) {
                            sMonth="0"+sMonth;
                        }
                        sDate = sYear+"-"+sMonth+"-"+sDay;
                        if (sDate==timeArr[i]){
                            $(this).addClass("active");
                            $(this).click(function(event){
                                var sMonth = parseInt(switchTimeArr[1]);
                                var sYear = parseInt(switchTimeArr[3]);
                                var sDay = $(this).html();
                                if(sDay.length == 1){
                                    sDay = 0 + sDay;
                                }
                                sMonth = sMonth - 1;
                                if (sMonth==0) {
                                    sMonth = 12;
                                    sYear = sYear - 1;
                                }
                                if (sMonth<10) {
                                    sMonth="0"+sMonth;
                                }
                                sDate = sYear+"-"+sMonth+"-"+sDay;
                                var a = $("<a href='http://search.sjtu.edu.cn/search-report/lectureSearch?searchStartDate="+sDate+"&searchEndDate="+sDate+"' target='_blank'></a>").get(0);
                                var e = document.createEvent("MouseEvents");
                                e.initEvent("click", true, true );
                                a.dispatchEvent(e);
                                event.stopPropagation();
                            });
                        }
                        return;
                    }
                    
                    if ($(this).hasClass("new")) {
                        sMonth = sMonth + 1;
                        if (sMonth==13) {
                            sMonth = 1;
                            sYear = sYear + 1;
                        }
                        if (sMonth<10) {
                            sMonth="0"+sMonth;
                        }
                        sDate = sYear+"-"+sMonth+"-"+sDay
                        if (sDate==timeArr[i]){
                            $(this).addClass("active");
                            $(this).click(function(event){
                                var sMonth = parseInt(switchTimeArr[1]);
                                var sYear = parseInt(switchTimeArr[3]);
                                var sDay = $(this).html();
                                if(sDay.length == 1){
                                    sDay = 0 + sDay;
                                }
                                sMonth = sMonth + 1;
                                if (sMonth==13) {
                                    sMonth = 1;
                                    sYear = sYear + 1;
                                }
                                if (sMonth<10) {
                                    sMonth="0"+sMonth;
                                }
                                sDate = sYear+"-"+sMonth+"-"+sDay;
                                var a = $("<a href='http://search.sjtu.edu.cn/search-report/lectureSearch?searchStartDate="+sDate+"&searchEndDate="+sDate+"' target='_blank'></a>").get(0);
                                var e = document.createEvent("MouseEvents");
                                e.initEvent("click", true, true );
                                a.dispatchEvent(e);
                                event.stopPropagation();
                            });
                        }
                        return;
                    }
                    
                    if (sMonth<10) {
                        sMonth="0"+sMonth;
                    }
                    sDate = sYear+"-"+sMonth+"-"+sDay;
                    if (sDate==timeArr[i]){
                        $(this).addClass("active");
                        $(this).click(function(event){
                            var sMonth = parseInt(switchTimeArr[1]);
                            var sYear = parseInt(switchTimeArr[3]);
                            var sDay = $(this).html();
                            if(sDay.length == 1){
                                sDay = 0 + sDay;
                            }
                            if (sMonth<10){
                                sMonth="0"+sMonth;
                            }
                            sDate = sYear+"-"+sMonth+"-"+sDay;
                            var a = $("<a href='http://search.sjtu.edu.cn/search-report/lectureSearch?searchStartDate="+sDate+"&searchEndDate="+sDate+"' target='_blank'></a>").get(0);
                            var e = document.createEvent("MouseEvents");
                            e.initEvent("click", true, true );
                            a.dispatchEvent(e);
                            event.stopPropagation();
                        });
                    }
                })
            }
        }
