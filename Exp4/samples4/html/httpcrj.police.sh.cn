                    var sub_icons_name_wsbz = name_wsbz + "-sub-icons";
                    var left_icon_name_wsbz = name_wsbz + "-zggm";
                    var right_icon_name_wsbz = name_wsbz + "-wgr";

                    this.getIconLayerName = function() {
                        return icon_name_wsbz;
                    }
                    this.getSubIconLayerGroupName = function() {
                        return sub_icons_name_wsbz;
                    }
                    layerStorage[bg_mk_name_wsbz] = this;

                    $("canvas").drawRect({ //背景层
                        layer: true,
                        name: bg_mk_name_wsbz,
                        fillStyle: "#2974ee",
                        x: 141 * scaleYZ + offsetX,
                        y: 66 * scaleYZ + offsetY,
                        width: 273 * scaleYZ,
                        height: 132 * scaleYZ,
                        mask: true,
                        mouseover: function(layer) {
                            if (!eventSign[layer.name]) {
                                mouseleave(layer.name);
                                eventSign[layer.name] = true;
                                $("canvas").stopLayer(icon_name_wsbz, true);
                                $("canvas").stopLayerGroup(sub_icons_name_wsbz, true);
                                $("canvas").animateLayer(icon_name_wsbz, {
                                    width: 346 * scaleYZ,
                                    height: 202 * scaleYZ,
                                    opacity: 0
                                }, function() {
                                    $("canvas").animateLayerGroup(sub_icons_name_wsbz, {
                                        y: 66 * scaleYZ + offsetY,
                                        opacity: 1
                                    });
                                });
                            }

                        },
                        mouseout: function(layer) {
                            //在进入图标层时避免重复触发该事件
                            if (eventSign[layer.name] === MOUSE_OUT || !checkPointInLayer(layer)) {
                                eventSign[layer.name] = false;
                                $("canvas").stopLayer(icon_name_wsbz, true);
                                $("canvas").stopLayerGroup(sub_icons_name_wsbz, true);
                                $("canvas").animateLayerGroup(sub_icons_name_wsbz, {
                                    y: -51 * scaleYZ + offsetY,
                                    opacity: 0,
                                    width: 60 * scaleYZ,
                                    height: 101 * scaleYZ
                                }, function() {
                                    $("canvas").animateLayer(icon_name_wsbz, {
                                        width: 173 * scaleYZ,
                                        height: 101 * scaleYZ,
                                        opacity: 1
                                    });
                                });
                            }
                        }
                    }).drawImage({ //图标层（网上办证，该层的所有事件被屏蔽）
                        layer: true,
                        name: icon_name_wsbz,
                        x: 141 * scaleYZ + offsetX,
                        y: 66 * scaleYZ + offsetY,
                        source: "imageNews/onlineBZ.svg",
                        width: 173 * scaleYZ,
                        height: 101 * scaleYZ,
                        opacity: 1,
                        intangible: true /*屏蔽当前层的事件，使其下方的背景层能触发鼠标进入、离开事件*/
                    }).drawImage({ //图标层（中国公民）
                        layer: true,
                        name: left_icon_name_wsbz,
                        groups: [sub_icons_name_wsbz],
                        source: "imageNews/zggm.svg",
                        width: 60 * scaleYZ,
                        height: 101 * scaleYZ,
                        x: 80 * scaleYZ + offsetX,
                        y: -51 * scaleYZ + offsetY,
                        opacity: 0,
                        mouseover: function(layer) {
                            $(this).stopLayer(icon_name_wsbz, true);
                            $(this).animateLayer(layer, {
                                width: 70 * scaleYZ,
                                height: 118 * scaleYZ
                            });
                           /* 20180908 update
                           $("canvas").animateLayerGroup("tsk_content", {
                                opacity: 1
                            });*/
                        },
                        mouseout: function(layer) {
                           /* 20180908 update
                            $("canvas").animateLayerGroup("tsk_content", {
                                opacity: 0
                            });*/
                            $(this).stopLayer(icon_name_wsbz, true);
                            $(this).animateLayer(layer, {
                                width: 60 * scaleYZ,
                                height: 101 * scaleYZ
                            });
                        },
                        click: function(layer) {
                            window.location.href = "/zggmCX.jsp";
                        } /*单击事件*/
                    }).drawImage({ //图标层（外国人）
                        layer: true,
