            <script type="text/javascript">
                //在鼠标离开事件中检测当前光标的位置是否在背景层的范围内
                //在背景层上会出现多个图标层，当鼠标进入图标层时会依次触发背景层的鼠标进入和离开事件
                //此时需要使用该方法进行判断，如果光标在背景层范围内，不要触发已有事件，否则图标层会被隐藏
                function checkPointInLayer(layer, dx, dy) {
                    var dx = dx || 10;
                    var dy = dy || dx;
                    var l = layer.x - layer.width / 2 + dx;
                    var t = layer.y - layer.height / 2 + dy;
                    var r = l + layer.width - dx;
                    var b = t + layer.height - dy;
                    var cx = layer.eventX,
                        cy = layer.eventY;
                    if (cx < l || cx > r || cy < t || cy > b)
                        return false;
                    return true;
                }
                //事件信号对象，以层名称作为KEY，VALUE为boolean值，避免事件重复触发
                //当层垒叠在一起时，鼠标从当前层移动到另一层时，也会触发上一层和下一层的鼠标进入或离开事件
                //事件信号的作用就是在多个层发生事件交叉传递时，控制事件发生的时序
                //类似于多线程的同步机制
                var MOUSE_OUT = 1;
                var eventSign = {};
                var layerStorage = {};

                function mouseleave(exceptName) {
                    exceptName = exceptName || "";
                    for (layerName in eventSign) {
                        if (layerName != exceptName && eventSign[layerName]) {
                            console.log("mouseleave->" + layerName + ":" + eventSign[layerName]);
                            eventSign[layerName] = MOUSE_OUT;
                            var layer = $("canvas").getLayer(layerName);
                            layer.mouseout(layer);
                        }
                    }
                }

                var scaleYZ = 0;
                $(document).ready(function() {
                    createCanvas();
                });

                $(window).bind("resize", function() {
                    location.reload();
                });


                function createCanvas() {
                    $("canvas").removeLayers();

                    canvas.width = document.body.clientWidth;
                    canvas.height = document.body.clientHeight;
                    var widthX = 1600;
                    var heightY = 1094;
                    scaleYZ = canvas.width / widthX;
                    var X = $(".bgimg3").width();
                    var tdId = $("#refContainer").height() * scaleYZ;
                    $("#refContainer").css("height", tdId);

                    var rightX = 772 + 66;
                    var leftX = 141 - 136.5;
                    var contentWidth = rightX - leftX;
                    var centX = (canvas.width - contentWidth * scaleYZ) / 2;

                    $("canvas").drawRect({
                        layer: true,
                        fromeCenter: false,
                        x: 0,
                        y: 0,
                        width: canvas.width,
                        height: canvas.height,
                        mousemove: mouseleave
                    });

                     var ref = $("#refContainer").offset(); createLayer({ name:
                            "wsbz", offsetX: centX, offsetY: ref.top });
                      createLayerBZCX({ name: "bzcx", offsetX: centX, offsetY:
                    ref.top + 140 * scaleYZ }); createLayerQYBA({ name: "qyba",
                      offsetX: 560 * scaleYZ + centX, offsetY: ref.top + 140 *
                     scaleYZ }); createLayerGFWB({ name: "gfwb", offsetX: -71 *
                       scaleYZ + centX, offsetY: ref.top + 280 * scaleYZ });
                      createLayerYQLJ({ name: "yqlj", offsetX: 280 * scaleYZ +
                            centX, offsetY: ref.top + 280 * scaleYZ });
                     createLayerSimple({ offsetX: centX, offsetY: ref.top }); }


              /***   第一块:网上办证      ***/
                function createLayer(params) {
                    var _params_wsbz = params || {};
                    var offsetX = _params_wsbz.offsetX || 0;
                    var offsetY = _params_wsbz.offsetY || 0;
                    var name_wsbz = _params_wsbz.name || "def";
                    var bg_mk_name_wsbz = name_wsbz + "-bg-mk";
