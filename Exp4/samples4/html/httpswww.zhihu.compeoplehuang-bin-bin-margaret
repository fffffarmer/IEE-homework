lowing&quot;:false,&quot;isOrg&quot;:false,&quot;gender&quot;:0,&quot;badge&quot;:[],&quot;id&quot;:&quot;671de4624f56f8fb191f44b9e01d4882&quot;},&quot;actionText&quot;:&quot;关注了问题&quot;,&quot;createdTime&quot;:1539941169,&quot;type&quot;:&quot;feed&quot;,&quot;id&quot;:&quot;1539941169&quot;},&quot;1539942625&quot;:{&quot;verb&quot;:&quot;QUESTION_FOLLOW&quot;,&quot;target&quot;:{&quot;id&quot;:294103231,&quot;schema&quot;:&quot;question&quot;},&quot;actor&quot;:{&quot;isFollowed&quot;:false,&quot;type&quot;:&quot;people&quot;,&quot;name&quot;:&quot;黄彬彬Margaret&quot;,&quot;headline&quot;:&quot;入心理学专业坑第七年了&quot;,&quot;urlToken&quot;:&quot;huang-bin-bin-margaret&quot;,&quot;userType&quot;:&quot;people&quot;,&quot;url&quot;:&quot;https://api.zhihu.com/people/671de4624f56f8fb191f44b9e01d4882&quot;,&quot;avatarUrl&quot;:&quot;https://pic3.zhimg.com/50/adac17cdc0b4cdbe9cd7c7e987048a15_s.jpg&quot;,&quot;isFollowing&quot;:false,&quot;isOrg&quot;:false,&quot;gender&quot;:0,&quot;badge&quot;:[],&quot;id&quot;:&quot;671de4624f56f8fb191f44b9e01d4882&quot;},&quot;actionText&quot;:&quot;关注了问题&quot;,&quot;createdTime&quot;:1539942625,&quot;type&quot;:&quot;feed&quot;,&quot;id&quot;:&quot;1539942625&quot;},&quot;1539943227&quot;:{&quot;verb&quot;:&quot;QUESTION_FOLLOW&quot;,&quot;target&quot;:{&quot;id&quot;:296674849,&quot;schema&quot;:&quot;question&quot;},&quot;actor&quot;:{&quot;isFollowed&quot;:false,&quot;type&quot;:&quot;people&quot;,&quot;name&quot;:&quot;黄彬彬Margaret&quot;,&quot;headline&quot;:&quot;入心理学专业坑第七年了&quot;,&quot;urlToken&quot;:&quot;huang-bin-bin-margaret&quot;,&quot;userType&quot;:&quot;people&quot;,&quot;url&quot;:&quot;https://api.zhihu.com/people/671de4624f56f8fb191f44b9e01d4882&quot;,&quot;avatarUrl&quot;:&quot;https://pic3.zhimg.com/50/adac17cdc0b4cdbe9cd7c7e987048a15_s.jpg&quot;,&quot;isFollowing&quot;:false,&quot;isOrg&quot;:false,&quot;gender&quot;:0,&quot;badge&quot;:[],&quot;id&quot;:&quot;671de4624f56f8fb191f44b9e01d4882&quot;},&quot;actionText&quot;