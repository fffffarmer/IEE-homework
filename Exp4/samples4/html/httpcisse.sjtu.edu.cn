    <script language="javascript" type="text/javascript">  
	//js截取字符串，中英文都能用  
	//如果给定的字符串大于指定长度，截取指定长度返回，否者返回源字符串。  
	//字符串，长度  
	  
	/** 
	 * js截取字符串，中英文都能用 
	 * @param str：需要截取的字符串 
	 * @param len: 需要截取的长度 
	 */ 
	 $(document).ready(function(e) {
        $('.jq').find('a').each(function(){
			var t=this
			if($(this).parent('li').find('img').attr('src')){
				var html=cutstr($(t).html(),24);
			}else
			{
				var html=cutstr($(t).html(),30);
			}
			$(t).html(html);
			})
    });
	function cutstr(str,len)  
	{  
	   var str_length = 0;  
	   var str_len = 0;  
		  str_cut = new String();  
		  str_len = str.length;  
		  for(var i = 0;i<str_len;i++)  
		 {  
			a = str.charAt(i);  
			str_length++;  
			if(escape(a).length > 4)  
			{  
			 //中文字符的长度经编码之后大于4  
			 str_length++;  
			 }  
			 str_cut = str_cut.concat(a);  
			 if(str_length>=len)  
			 {  
			 str_cut = str_cut.concat("...");  
			 return str_cut;  
			 }  
		}  
		//如果给定字符串小于指定长度，则返回源字符串；  
		if(str_length<len){  
		 return  str;  
		}  
	}  
	  
	</script> 
