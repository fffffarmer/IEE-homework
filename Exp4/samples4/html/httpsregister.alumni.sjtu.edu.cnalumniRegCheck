








<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>校友注册</title>
    
<title>上海交通大学校友统一身份认证</title>
<script src="https://register.alumni.sjtu.edu.cn/UI/jquery/jquery-1.8.3.min.js" type="text/javascript"></script>
	<script type="text/javascript">
		$(document).ready(function(){
			var regNotice = $("#regNotice").html();
			if(regNotice=="to:reg1"){
				parent.location.replace("https://register.alumni.sjtu.edu.cn/alumniReg-1");
			}else if(regNotice=="to:reg2"){
				parent.location.replace("https://register.alumni.sjtu.edu.cn/alumniReg-2");
			}else if(regNotice=="to:reg3"){
				parent.location.replace("https://register.alumni.sjtu.edu.cn/alumniReg-3");
			}else if(regNotice=="to:regtran"){
				parent.location.replace("https://register.alumni.sjtu.edu.cn/alumniRegTran");
			}else if(regNotice=="to:regJacPage"){
				parent.location.replace("https://register.alumni.sjtu.edu.cn/alumniRegJac");
			}else{
				parent.showNotice(regNotice);
				$("input[class*=regNext]", window.parent.document).removeAttr("disabled");
			}
		});
	</script>
  </head>
  <body>
  <div id="regNotice">to:reg1</div>
  </body>
</html>
