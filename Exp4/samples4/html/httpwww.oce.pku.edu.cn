<script type="text/javascript">
function searchPorjects(val){
	var contentCategory ;
	//项目类别
	if(val == ''){
		contentCategory = $('.dandu_5_on').find('a').attr('value');
	}else{
		contentCategory = $(val).find('a').attr('value');
		$(val).addClass('dandu_5_on').siblings().removeClass('dandu_5_on');
		$(val).find('a').removeClass().addClass('bububu_1').parent().siblings().find('a').removeClass().addClass('bububu');
	}
	//招生范围，1：公开项目；2：内训项目
	var range = $('#range').val();
	//项目名称
	var contentName = $('#contentName').val();
	
	$.ajax({
		type:'post',
		dataType:"json",
		url:'/searchPorjects',
		//contentType:'application/json;charset=utf-8',//请求是key/value这里不需要指定contentType，因为默认就 是key/value类型
		data:'contentCategory='+contentCategory+'&range='+range+'&contentName='+contentName,
		success:function(data){//返回json结果
			var trainStr = '';
			for (var i in data) {
				trainStr +='<div class="dand_1">'
						 + '<div class="dandu_1"><a style="color:#666" href="/train/'+ cutoutStr(data[i].contentCategory, 4) + ((data[i].contentRange == 0) ? 'ack8ej' : 'bck8ej') +'/'+ data[i].projectId +'" class="co_black">'+ abbreviate(data[i].contentName,50) +'</a></div>'
						 + '<div class="dandu_2">'+ data[i].contentOrg +'</div>'
						 + '<div class="dandu_3">'+ formatDate('y-m-d',data[i].contentPlanStart) +'</div>'
						 + '</div>';
			}
			$('#trainingDiv').html(trainStr);
		}
	});
	
}

$(function() {
	searchPorjects('');
});
</script>
