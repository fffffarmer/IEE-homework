			for (var i in teachingFacultys) {
				teachingFacultyStr += '<div class="six">'
									+ '<a target="_blank" href="'+ teachingFacultys[i].contentLink +'" class="six_1 co_black">'+ teachingFacultys[i].contentName +'</a>'
									+ '</div>';
			}
			$("#teachingFacultyDiv").html(teachingFacultyStr);
			
		}
	});
	
	
	
	$.ajax({
		type:'post',
		dataType:"json",
		url:'/selectByOrganizeType',
		data:'projectCustom.range='+ $('#range').val() ,
		contentType:'application/json;charset=utf-8',
		success:function(data){
			//非学历教育教学院系
			var nonDegreeFacultyStr = '<div class="Columnn"><a class="a_1">非学历教育教学院系</a> <a class="a_2">Non Degree Faculty</a></div>';
			for(var i=0; i<data.length;i++){
				var deleteObj = data[i];
				if(deleteObj.organizeId == '6' || deleteObj.organizeId == '42' || deleteObj.organizeId == '57'){
					data.splice(i,1);
				}
			}
			for (var i in data) {
					if(i == 0){
						nonDegreeFacultyStr += '<div class="ssss">';
					}else if(i%5 == 0  && i!= 0){
						nonDegreeFacultyStr += '</div><div class="ssss">';
					}
					nonDegreeFacultyStr += '<div class="ssss_1"><a target="_blank" href="'+ data[i].website +'" title="'+ data[i].organizeName +'&#10;联系人：'+ data[i].applyContacter +'&#10;联系电话：'+ data[i].applyTel +'&#10;办公地址：'+ data[i].workAddr +'&#10;官方网址：'+ data[i].website +'" class="ssssa co_black">'+ abbreviate(data[i].organizeName,7) +'</a></div>';
					if(i == 44) break ;
				}
			nonDegreeFacultyStr +='</div>';
			$("#nonDegreeFacultyDiv").html(nonDegreeFacultyStr);
		}
	});
});

</script>
</body>
</html>
