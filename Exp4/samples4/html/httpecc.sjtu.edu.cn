        <script type="text/javascript">
            var i = 0, count;
            var _adObj;
            $(document).ready(function () {
                $("#course").hide();
                count = $("#banner .ul").length;
                $("#banner .ul:not(:first-child)").hide();
                $(".changepic").hover(function () {
                    $(this).addClass("picselect");
                }, function () {
                    $(this).removeClass("picselect");
                });
                $(".changepic").click(function () {
                    clearTimeout(_adObj);
                    changepic($(this).attr("id"));
                });
                $(".ban").hover(function () {
                    $(".changepic").show();
                }, function () {
                    $(".changepic").hide();
                });
                setTimeout(function () {
                    changepic("toleft");
                }, 5000);
            });
            function changepic(id)
            {
                if (id == "toleft") {
                    if (i == 0)
                        i = count;
                    i--;
                }
                if (id == "toright") {
                    if (i == count - 1)
                        i = -1;
                    i++;
                }
                $("#banner .ul").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn();
                $("#banner .ul").parent().fadeIn(3000);
                _adObj = setTimeout(function () {changepic("toleft");}, 5000);
            }
        </script>
