<script type="text/javascript">
    var default_name="Account Number";
    var default_pwd = "Password";
    //$(document).keypress(function (e) {
    //    if (e.which == 13) {
    //        $("#login").click();
    //    }
    //});

    $(function () {
        $("#name").val(default_name).keyup();
        $("#name").focus(function () {
            if ($(this).val() == default_name) {
                $(this).val("").keyup();
            }
        }).blur(function () {
        });

        $("#pwd").val(default_pwd).keyup();
        $("#pwd").focus(function () {
            $(this).css("display", "none");
            $("#pwd1").css("display", "block");
            $("#pwd1").focus();
        });
        $(".indexlogin_li4").click(function () {
            $(".indexlogin").slideUp("fast");
            $("#head").removeClass("headbj");
            $("#name").val(default_name);
            $("#pwd").val(default_pwd);
            $("#pwd1").val("");
            $("#pwd").css("display", "block");
            $("#pwd1").css("display", "none");
            $("#login").html("log in");
            $("#login").removeClass("signout").removeClass("signin").addClass("alogin");
        });
    });

    function parseName(obj) {
        if (obj.val() == "" || obj.val() == default_name) {
            setFocus("Missing account number. Please correct and try again.");
            return false;
        }
        else {
            return true;
        }
    }

    function parsePwd(obj) {
        if (obj.val() == "" || obj.val() == default_pwd) {
            setFocus("Please enter your password.");
            return false;
        } else
            return true;
    }

    function setFocus(tips)
    {
        $("#tip").show();
        $("#tip").html(tips);
    }
</script>

<script type="text/javascript">
    var aclass = 'alogin';
    var issign = false;
    $(function () {
        var admintype = '0';
        $("#login").addClass(aclass);
        if(aclass=="signout")
        {   
            issign=true;
            $("#login").removeClass("alogin").removeClass("signin").addClass("signout");
            $("#login").html("sign out");
            if (admintype==3)
            {
                $("#user").html("Welcome,"+"<a href=\"/user/index.aspx\" target=\"_blank\" class=\"headuser\">[Guest]</a>");
            } else if (admintype == 4) {
                $("#user").html("Welcome," + "<a href=\"javascript:\" onclick=\"showaccount();\"  class=\"headuser\">[Guest]</a>");
            }
        }
    });
    $("#login").click(function ()
    {   
        $("#tip").html("");
        if ($(this).hasClass("alogin")) {
            $("#head").addClass("headbj");
            $(".indexlogin").slideDown("slow");
            $("#login").html("sign in");
            $(this).removeClass("alogin").removeClass("signout").addClass("signin");
        }
        else if (!issign)
        {
            var _submitstatus = 0;
            if (_submitstatus == 1)
                return false;

            var _return = true;

            if (!parseName($("#name")))
                _return = false;
            else if (!parsePwd($("#pwd1")))
                _return = false;

            if (_return) {
                _submitstatus = 1;

                var _dataobj = new Object;
                _dataobj.name = $("#name").val();
                _dataobj.pwd = $("#pwd1").val();

                $.ajax({
                    type: "get",
                    dataType: "",
                    url: "/tools/userlogin.ashx",
                    data: { name: _dataobj.name, pwd: _dataobj.pwd},
                    success: function (data) {
                        var datas = data.split('^');
                        if(datas.length>0){
                            switch (datas[0]) {
                                case "-1":
                                    setFocus("Missing account number. Please correct and try again.");
                                    $("#name").focus();
                                    break;
                                case "-2":
                                    setFocus("Please enter your password.");
                                    $("#pwd1").focus();
                                    break;
                                case "-3":
                                    setFocus("There was an error with your Account/Password combination. Please try again.");
                                    $("#pwd1").focus();
                                    break;
                                case "1":
                                    $("#login").removeClass("signin").removeClass("alogin").addClass("signout");
                                    $(".indexlogin").slideUp("fast");
                                    $("#login").html("sign out");
                                    var admintype = datas[1];
                                    if (admintype==3)
                                    {
                                        $("#user").html("Welcome,"+"<a href=\"/user/index.aspx\" target=\"_blank\" class=\"headuser\">["+datas[2]+"]</a>");
                                    }else
                                        $("#user").html("Welcome," + "<a href=\"javascript:\" onclick=\"showaccount();\" class=\"headuser\">[" + datas[2] + "]</a>");
                                    $("#head").removeClass("headbj");
                                    issign=true;
                                    break;
                                case "-4":
                                    setFocus("Account number contains illegal characters . Please re-type.");
                                    $("#name").focus();
                                    break;
                                case "-5":
                                    setFocus("Password contains illegal characters . Please re-type.");
                                    $("#name").focus();
                                    break;
                                case "-6":
                                    setFocus("Your Account number has been locked.");
                                    $("#name").focus();
                                    break;
                                case "-7":
                                    setFocus("Your account has been verified many times today please try again after a hour");
                                    $("#name").focus();
                                    break;
                                default:
                                    setFocus("Error.");
                                    break;
                            }
                        }

                        _submitstatus = 0;
                    }
                });
            }
        }
        else if (issign)
        {
            var _submitstatus = 0;
            if (_submitstatus == 1)
                return false;
            $.ajax({
                type: "get",
                dataType: "",
                url: "/tools/userloginout.ashx",
                success: function (data) {
                    _submitstatus = 0;
                    $("#login").removeClass("signout").addClass("alogin");
                    $("#name").val(default_name);
                    $("#pwd").val(default_pwd);
                    $("#pwd1").val("");
                    $("#pwd").css("display", "block");
                    $("#pwd1").css("display", "none");
                    $("#login").html("log in");
                    $("#tip").html("");
                    $("#user").html("Welcome, Guest");
                    $(".indexlogin").hide();
                    issign=false;
                }
            });
        }
    });
    function showaccount()
    {
        $.dialog({
            id: 'account',
            title: 'Your Account',
            content: 'url:/html/account.html',
            width: 640,
            max: false,
            min: false,
            fixed: true,
            lock: true,
            drag: false,
            resize: false,
            zIndex: 99999
        });
    }
</script>
