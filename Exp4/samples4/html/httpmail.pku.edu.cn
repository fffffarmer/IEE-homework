<script language="javascript">
function page_onLoad(){var une=document.getElementById("username");var _uck=getCookie("uid");if(_uck){une.value=_uck;document.getElementById('password').focus();}else{if(une.value==""){une.value="校园网帐号/北大邮箱";une.style.fontSize='14px';une.style.color='#999';une.focus();var iu = une.createTextRange();iu.collapse();iu.select();}}}
function loginchk(a,b){
if(document.getElementById('username').value==""||document.getElementById('username').value=="校园网帐号/北大邮箱")
{alert("缺少用户账号，请重新输入\n NetID or Mail address needed");document.getElementById('username').focus();return false;}if(document.getElementById('password').value==""){alert("缺少密码，请重新输入\n Password needed");document.getElementById('password').focus();return false;}
if(document.getElementById('verifyCode')!=null){if(document.getElementById('verifyCode').value==""){alert("请输入验证码\nVerifyCode needed");document.getElementById('verifyCode').focus();return false}
}document.getElementById('uid').value=document.getElementById('username').value;return loginSubmit(a,b);}
function gotoPWD(evt){evt=(evt)?evt:((window.event)?window.event:"");var key=evt.keyCode?evt.keyCode:evt.which;if(key==13){document.getElementById('password').focus();return false;}}
</script>
