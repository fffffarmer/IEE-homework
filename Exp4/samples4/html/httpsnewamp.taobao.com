        console.log(1)
        page++;
        if(page == len){
            page = 0;//当page大于图片长度时，从第一个图片开始播放
            $(".slider_xll").animate({"left":"0"},500);
        }
        $(".slider_xll").animate({"left":"-"+page*1110+"px"},500);

