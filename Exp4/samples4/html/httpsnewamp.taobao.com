    var page = -1;//设置开始轮播第一个图片
    var len = parseInt($('.ps').html());//设定轮播屏数
    var stop = false;//设置轮播是否停止


    /**定义轮播函数**/
    function slide(){
        if(!stop){
            page++;//当前轮播加1（下一个图片显示）
            if(page == len){
                page = 0;//当page大于图片长度时，从第一个图片开始播放
                $(".slider_xll").animate({"left":"0"},500);
            }
            $(".slider_xll").animate({"left":"-"+page*1110+"px"},500);
        }
        setTimeout(slide,3000);


    }
