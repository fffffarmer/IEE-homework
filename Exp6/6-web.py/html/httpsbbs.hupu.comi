    	var loginJs    = 'https://passport.hupu.com/pc/js/app/login.js';
    	var mode  = '';
    	var url   = (mode == 'mobile' ? registerJs : loginJs);

	    require(['https://passport.hupu.com/pc/js/common.js?v=2018052101'], function(commmon) {
            require([url]);
