      if(0>0){
        if(postdate>0){
          harf='lurenwangnews-type0-postdate'
        }else{
          harf='lurenwangnews-type0'
        }
      }else{
        if(postdate>0){
          harf='lurenwangnews-postdate'
        }else{
          harf='lurenwangnews'
        }
      }

    }
