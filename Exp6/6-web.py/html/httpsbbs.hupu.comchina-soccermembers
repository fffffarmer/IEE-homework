            function findPos_top(obj) {
                var top = obj.offsetTop || 0;
                while (obj = obj.offsetParent) {
                    top += obj.offsetTop;
                }
                return top;
            }

            function findPos_left(obj) {
                var l = obj.offsetLeft;
                while (obj.offsetParent != null) {
                    obj = obj.offsetParent;
                    l += obj.offsetLeft;
                }
                return l;
            }

            ifSideSticky = (IE6 == 'sucks') ? false : true;
            var pubPos = findPos_top($("#bbs_banner")[0]) - findPos_top($("#sidenav_bbs")[0]) + 403;
