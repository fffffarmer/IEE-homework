    $('.page').createPage(function(n){

    },{
    	pageCount:maxpage,//总页码,默认10
    	current:1,//当前页码
      name:harf+'-',//标记
      hname:'qanda',
    	showNear:1,//显示当前页码前多少页和后多少页，默认2
    	pageSwap:true,
      align:'right',
    	showSumNum:false,//是否显示总页码
      maxpage:100,
      is_poslist:1,
      is_read:0

    });
    $(".nextPage").css({
      "height":"16px",
      "line-height":"16px"
    });
  });window.initEvent && window.initEvent();</script></html>
