//搜索
$(".it-search-for").bind('input propertychange', function() {
    var $value=$(".it-search-for").val();
  if($value.length>0){
      $(".hp-search-choose").show();
      $(".searchval").html($value);
  }else{
      $(".hp-search-choose").hide();
  }
});
    //搜索按钮
    $(".btn-searchs").click(function(){
     var $value=$(".it-search-for").val();
      window.open('//my.hupu.com/search?q='+$value,'_blank');
    })
      // 回车键事件
    $(".it-search-for").keypress(function(e) {
       if(e.which == 13) {
         var $value=$(".it-search-for").val();
        window.open('//my.hupu.com/search?q='+$value,'_blank');
       }
   });
   //帖子
   $("#tiezi").click(function(){
     var $value=$(".it-search-for").val();
      window.open('//my.hupu.com/search?q='+$value,'_blank');
    })
  //视频
  $("#videos").click(function(){
    var $value=$(".it-search-for").val();
     window.open('//v.liangle.com/nba/s_'+$value,'_blank');
   })
   //日志
   $("#rizhi").click(function(){
     var $value=$(".it-search-for").val();
      window.open('https://my.hupu.com/search/blog-'+$value+'-5000','_blank');
    })
  //成员
  $("#cyuan").click(function(){
