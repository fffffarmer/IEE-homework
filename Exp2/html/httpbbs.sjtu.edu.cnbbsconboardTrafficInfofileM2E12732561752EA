function jumpUrl(x){
	var url = '', e = encodeURIComponent, s = screen, d = document, title = d.title, u = d.location;
	if(x=='douban'){
		url ="http://www.douban.com/recommend/?url="+e(u.href)+"&title="+e(title);
	}else if(x=='renren'){
		url ="http://share.renren.com/share/buttonshare?link="+e(u.href)+"&title="+e(title);
	}else if(x=='kaixin'){
		url ="http://www.kaixin001.com/repaste/share.php?rurl="+e(u.href)+"&rtitle="+e(title);
	}else if(x=='qzone'){
		url ="http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?to=xiaoyou&url="+e(u.href);
	}else if(x=='sina'){
		url ="http://v.t.sina.com.cn/share/share.php?title="+e(title)+"&url="+e(u.href);
	}
	function a(){
		if (!window.open(url))
			u.href = [url].join('');
	}
	if (/Firefox/.test(navigator.userAgent)){
		setTimeout(a, 0);
	}else{
		a();
	}
}
</script>
